{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowS3WriteToProcessedFolder",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::ecommerce-pipeline-godson/processed/*"
    },
    {
      "Sid": "AllowS3ListBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::ecommerce-pipeline-godson"
    }
  ]
}